#include<iostream>
#include<string.h>
using namespace std ; 
int main(){
    int i=0;
    char temp;
string s;
getline(cin,s);


for (int j = 0; j< s.length(); j++)
{
    if ((s[j]-s[j+1])!=0)
    {
       temp=s[i];
     s[i]=s[i+1];
     s[i+1]=temp;
      cout<<s[i]<<endl;
     i++;
    }
    
}
for (int i = 0; i< 5; i++)
{
    cout<<s[i];
}
return 0;
}
